<div id="<%= dom_id parking_lot %>">
  <p>
    <b> Parking <%= parking_lot.id %> </b>
    <br/>
    Maximim spots: <%= parking_lot.max_spots %>
    <br/>
    <%= button_to 'View Entrances', parking_lot_entrances_url(parking_lot), method: :get %>
    <br/>
    <%= button_to 'View Spots', parking_lot_spots_url(parking_lot), method: :get %>
    <br/>
    <%= button_to "View tickets", parking_lot_tickets_path(@parking_lot), method: :get %>
  </p>
</div>
